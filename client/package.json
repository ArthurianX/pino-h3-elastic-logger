{
  "name": "pino-logger-client",
  "version": "0.1.5",
  "description": "Logger client that sends logs to an H3 powered API (and to ElasticSearch) and console in development mode. This is part of [pino-h3-elastic-logger](https://github.com/ArthurianX/pino-h3-elastic-logger)}",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "types": "./dist/pino-logger-client.d.ts",
    "require": "./dist/pino-logger-client.js",
    "default": "./dist/pino-logger-client.mjs"
  },
  "types": "dist/pino-logger-client.d.ts",
  "main": "./dist/pino-logger-client.cjs",
  "module": "./dist/pino-logger-client.module.js",
  "unpkg": "./dist/pino-logger-client.umd.js",
  "scripts": {
    "build": "microbundle --define process.env.NODE_ENV=production",
    "dev": "microbundle --define process.env.NODE_ENV=development watch",
    "build-dev": "microbundle --define process.env.NODE_ENV=development",
    "example-dev": "npm run dev && serve ./ -p 3001",
    "example-prod": "npm run build && serve ./ -p 3001"
  },
  "author": "Arthur Kovacs",
  "email": "kovacs.arthur.kraid@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "npm-dts": "^1.3.12",
    "serve": "^14.1.2"
  }
}
