{
  "name": "pino-logger-client",
  "version": "0.1.11",
  "description": "Logger client that sends logs to an H3 powered API (and to ElasticSearch) and console in development mode. This is part of [pino-h3-elastic-logger](https://github.com/ArthurianX/pino-h3-elastic-logger)}",
  "author": "Arthur Kovacs",
  "email": "kovacs.arthur.kraid@gmail.com",
  "license": "ISC",
  "main": "dist/pino-logger-client.js",
  "module": "dist/pino-logger-client.mjs",
  "typings": "dist/pino-logger-client.d.ts",
  "scripts": {
    "build-dev": "NODE_ENV=development rollup -c",
    "build-prod": "NODE_ENV=production rollup -c",
    "dev": "npm run build-dev && serve ./ -p 3001",
    "just-serve": "serve ./ -p 3001"
  },
  "devDependencies": {
    "@types/node": "18.11.11",
    "esbuild": "0.16.1",
    "rollup": "2.79.1",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "serve": "^14.1.2",
    "typescript": "4.9.3"
  }
}
